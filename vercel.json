{
  "version": 2,
  "builds": [
    { "src": "api/api.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/api.js" },
    { "src": "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|ico|json)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
